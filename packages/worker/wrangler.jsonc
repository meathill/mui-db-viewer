{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "worker",
  "main": "src/index.ts",
  "compatibility_date": "2025-12-02",
  "compatibility_flags": ["nodejs_compat"],
  "assets": {
    "binding": "ASSETS",
    "directory": "./public"
  },
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "mui-db",
      "database_id": "cb8434c8-4537-481e-94ad-f82ab9917b41"
    }
  ],
  "observability": {
    "enabled": true
  },
  "placement": {
    "mode": "smart"
  },
  "vars": {
    "OPENAI_API_KEY": "",
    "OPENAI_BASE_URL": "",
    "OPENAI_MODEL": "gpt-5.2",
    "GEMINI_API_KEY": "",
    "GEMINI_MODEL": "gemini-3-pro-preview",
    "REPLICATE_API_KEY": "",
    "REPLICATE_MODEL": ""
  }
}
